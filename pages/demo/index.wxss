/* pages/demo/index.wxss */
@import '../Filter/filter.wxss';
.city-list{
  margin-top: 8rpx;
  padding: 1px 30rpx 30rpx;
  border-radius: 12rpx;
  background: #F8F8F8;
}
.city-list .city-item{
  margin-top: 30rpx;
  display: flex;
}
.city-list .city-info{
  display: flex;
  flex-direction: column;
  flex: 1;
  color: #666666;
}
.city-list .city-info .addr{
  margin-top: 12rpx;
  font-size: 24rpx;
  color: #999999;
}
.city-list .xzicon-position{
  margin: 5rpx 15rpx 0 0;
  color: #cccccc;
  font-size: 32rpx;
}